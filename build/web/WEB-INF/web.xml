<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>FacultyServices</servlet-name>
        <servlet-class>com.vahinfosol.Service.FacultyServices</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FacultyService</servlet-name>
        <servlet-class>com.vahinfosol.service.FacultyService</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>com.vahinfosol.service.Registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.vahinfosol.service.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addtask</servlet-name>
        <servlet-class>com.vahinfosol.service.addtask</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionService</servlet-name>
        <servlet-class>com.vahinfosol.service.SessionService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FacultyServices</servlet-name>
        <url-pattern>/FacultyServices</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FacultyService</servlet-name>
        <url-pattern>/FacultyService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addtask</servlet-name>
        <url-pattern>/addtask</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessionService</servlet-name>
        <url-pattern>/SessionService</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
